{
  "$schema": "http://json.schemastore.org/template",
  "author": "LiuliuSoft",
  "classifications": [ "Web", "OSharp" ],
  "name": "OSharp Template",
  "identity": "OSharp.Template.WebApi",
  "groupIdentity": "OSharp.Template.WebApi",
  "shortName": "osharp",
